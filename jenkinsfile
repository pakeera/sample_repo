pipeline{
 agent any
 stages{
   stage('echo'){
     steps{
      echo 'Hello world'
     }
   }
   stage(echo2){
   steps{
     echo 'this is my 1st pipeline using jenkinsfile'
   }
   }
   stage('Build') {
    steps {
      sh 'echo "Step 1"'
      sh 'echo "Step 2"'
      sh 'echo "Step 3"'
    }
  }
 }
}
